{
  "name": "lynkable",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:pwa": "NODE_ENV=production next build",
    "build:clean": "rm -rf .next && npm run build",
    "build:fast": "next build --debug",
    "analyze": "ANALYZE=true npm run build",
    "pwa:check": "node scripts/check-pwa.js",
